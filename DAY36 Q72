// Find the sum of all elements in a matrix

#include <stdio.h>
int main(void){
    int r,c,i,j,sum=0;
    printf("Enter rows and cols: ");
    scanf("%d%d",&r,&c);
    int a[r][c];
    for(i=0;i<r;i++)
        for(j=0;j<c;j++){
            scanf("%d",&a[i][j]);
            sum+=a[i][j];
        }
    printf("Sum = %d",sum);
    return 0;
}
