// Check if a matrix is symmetric

#include <stdio.h>
int main(void)
{
    int n,i,j,flag=1;
    printf("Enter size: ");
    scanf("%d",&n);
    int a[n][n];
    for(i=0;i<n;i++)
        for(j=0;j<n;j++)
            scanf("%d",&a[i][j]);
    for(i=0;i<n;i++)
        for(j=0;j<n;j++)
            if(a[i][j]!=a[j][i]) flag=0;
    if(flag) 
      printf("Matrix is symmetric");
    else
      printf("Matrix is not symmetric");
    return 0;
}
